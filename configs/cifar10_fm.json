{
  "dataset": "cifar10",
  "diffusion": {
    "timesteps": 1000,
    "beta_start": 0.0001,
    "beta_end": 0.02,
    "beta_schedule": "quad",
    "model_mean_type": "v",
    "model_var_type": "fixed-large",
    "loss_type": "flow_matching"
  },
  "model": {
    "in_channels": 3,
    "hid_channels": 128,
    "ch_multipliers": [1, 2, 2, 2],
    "num_res_blocks": 2,
    "apply_attn": [false, true, false, false],
    "drop_rate": 0.1
  },
  "train": {
    "model_lr": 2e-4,
    "value_lr": 1e-3,
    "policy_lr": 1e-2,
    "batch_size": 128,
    "grad_norm": 1.0,
    "epochs": 3,
    "warmup": 5000,
    "warmup_v": 0,
    "warmup_pi": 0,
    "use_ema": true,
    "ema_decay": 0.9999,
    "image_intv": 1,
    "chkpt_dir": "./chkpts/fm_comparison",
    "chkpt_intv": 1,
    "num_samples": 64,
    "use_baseline": false,
    "alg": "flow_matching",
    "max_buffer_length": 5,
    "n_features_to_select": 3,
    "update_policy": 40,
    "ent_coef": 1e-2,
    "clip_ratio": 1e-3
  },
  "ActorNetwork":{
    "input_channels": 3,
    "hidden_dim": 256,
    "hidden_depth": 2,
    "initialize": true    
  },
  "ValueNetwork":{
    "input_channels": 3,
    "hidden_dim": 256,
    "hidden_depth": 4,
    "initialize": false    
  }
}
